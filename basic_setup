--
-- Name: raise_topic_status_updates_readonly(); Type: FUNCTION; Schema: discourse_functions; Owner: cristinarotaru
--

CREATE FUNCTION discourse_functions.raise_topic_status_updates_readonly() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
  BEGIN
    RAISE EXCEPTION 'Discourse: topic_status_updates is read only';
  END
$$;


ALTER FUNCTION discourse_functions.raise_topic_status_updates_readonly() OWNER TO cristinarotaru;

