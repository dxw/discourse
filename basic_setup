CREATE TABLE public.web_hook_events (
    id integer NOT NULL,
    web_hook_id integer NOT NULL,
    headers character varying,
    payload text,
    status integer DEFAULT 0,
    response_headers character varying,
    response_body text,
    duration integer DEFAULT 0,
    created_at timestamp without time zone NOT NULL,
    updated_at timestamp without time zone NOT NULL
);


ALTER TABLE public.web_hook_events OWNER TO cristinarotaru;

--
-- Name: web_hook_events_id_seq; Type: SEQUENCE; Schema: public; Owner: cristinarotaru
--

CREATE SEQUENCE public.web_hook_events_id_seq
    AS integer
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.web_hook_events_id_seq OWNER TO cristinarotaru;

--
-- Name: web_hook_events_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: cristinarotaru
--

ALTER SEQUENCE public.web_hook_events_id_seq OWNED BY public.web_hook_events.id;


--
-- Name: web_hooks; Type: TABLE; Schema: public; Owner: cristinarotaru
--

CREATE TABLE public.web_hooks (
    id integer NOT NULL,
    payload_url character varying NOT NULL,
    content_type integer DEFAULT 1 NOT NULL,
    last_delivery_status integer DEFAULT 1 NOT NULL,
    status integer DEFAULT 1 NOT NULL,
    secret character varying DEFAULT ''::character varying,
    wildcard_web_hook boolean DEFAULT false NOT NULL,
    verify_certificate boolean DEFAULT true NOT NULL,
    active boolean DEFAULT false NOT NULL,
    created_at timestamp without time zone NOT NULL,
    updated_at timestamp without time zone NOT NULL
);


ALTER TABLE public.web_hooks OWNER TO cristinarotaru;

--
-- Name: web_hooks_id_seq; Type: SEQUENCE; Schema: public; Owner: cristinarotaru
--

CREATE SEQUENCE public.web_hooks_id_seq
    AS integer
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.web_hooks_id_seq OWNER TO cristinarotaru;

--
-- Name: web_hooks_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: cristinarotaru
--

ALTER SEQUENCE public.web_hooks_id_seq OWNED BY public.web_hooks.id;


